                    if (input.value.trim()) {
                      handleChatMessage(input.value);
                      input.value = '';
                    }
                  }}
                
                  className="bg-green-600 hover:bg-green-700 text-white px-4 py-3 rounded-lg transition-all"
                >
                  Send
                </button
                  className="bg-green-600 hover:bg-green-700 text-white px-4 py-3 rounded-lg transition-all"
                >
              </div
                  className="bg-green-600 hover:bg-green-700 text-white px-4 py-3 rounded-lg transition-all"
                >
            </div
                  className="bg-green-600 hover:bg-green-700 text-white px-4 py-3 rounded-lg transition-all"
                >
          </div
                  className="bg-green-600 hover:bg-green-700 text-white px-4 py-3 rounded-lg transition-all"
                >
        </div
                  className="bg-green-600 hover:bg-green-700 text-white px-4 py-3 rounded-lg transition-all"
                >
      )}
    </div
                  className="bg-green-600 hover:bg-green-700 text-white px-4 py-3 rounded-lg transition-all"
                >
  );
};

export default UnifiedPricingPageV2; 